<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.aimbrain.bankingdemo" >

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.GET_ACCOUNTS" />

    <application
        android:name="com.aimbrain.bankingdemo.MyApplication"
        android:allowBackup="true"
        android:icon="@drawable/launch_icon"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.aimbrain.bankingdemo.MainActivity"
            android:label="MainActivity"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name="com.aimbrain.bankingdemo.SignInActivity"
            android:label="@string/title_activity_sign_in"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name="com.aimbrain.bankingdemo.DemoBankActivity"
            android:label="@string/title_activity_demo_bank"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name="com.aimbrain.bankingdemo.AccountDetailsActivity"
            android:label="@string/title_activity_account_details"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name="com.aimbrain.bankingdemo.PaymentActivity"
            android:label="@string/title_activity_payment"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name="com.aimbrain.bankingdemo.TransactionsActivity"
            android:label="@string/title_activity_transactions"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name="com.aimbrain.bankingdemo.LauncherActivity"
            android:label="@string/app_name"
            android:screenOrientation="portrait" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.aimbrain.sdk.faceCapture.PhotoFaceCaptureActivity"/>
        <activity android:name="com.aimbrain.sdk.faceCapture.VideoFaceCaptureActivity"/>

        <activity
            android:name="com.aimbrain.bankingdemo.EmailConfirmationActivity"
            android:label="@string/title_activity_email_confirmation" >
        </activity>
    </application>

</manifest>
